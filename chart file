int W (int i, int j)  {
if (i==j)
   return alpha[j]; 
	else 
	return W (i,j-1) + alpha [j];
}
int c (int i, int j) {
int C (int i , int j ) [
if  (int i == j) {
return 0;
else {
int mincost =maxine; 
int cost;
for (int k= i=1; k<=j; k++) {
cost = C(i,k-1) + C (k,j) + W (i,j);
if (cost <mincost) 
     mincosst = cost ;
	}
	  return mincost;
	}
}
